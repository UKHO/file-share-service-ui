{
  "name": "ukho-fileshareservice-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --testPathPattern=Tests/UnitTests/",
    "test:auto": "jest --testPathPattern=Tests/FunctionalTests/",
    "test:a11y": "jest --testPathPattern=Tests/AccessibilityTests/",
    "lint": "ng lint",
    "preinstall": "npx npm-force-resolutions"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "outputDirectory": ".",
    "outputName": "junit.xml"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "projects": [
      "<rootDir>/Tests/UnitTests/jest.config.js",
      "<rootDir>/Tests/FunctionalTests/jest.config.js",
      "<rootDir>/Tests/AccessibilityTests/jest.config.js"
    ],
    "testTimeout": 120000
  },
  "resolutions": {
    "dns-packet": "~5.2.2",
    "postcss": "~8.2.10",
    "ws": "~7.4.6",
    "glob-parent": "~5.1.2",
    "chokidar": "~3.5.1",
    "css-what": "^5.0.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.13",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "~11.2.13",
    "@angular/compiler": "~11.2.13",
    "@angular/core": "~11.2.13",
    "@angular/forms": "~11.2.13",
    "@angular/platform-browser": "~11.2.13",
    "@angular/platform-browser-dynamic": "~11.2.13",
    "@angular/router": "~11.2.13",
    "@azure/msal-angular": "^2.0.0",
    "@azure/msal-browser": "^2.14.2",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ukho/design-system": "^2.1.0",
    "jest-junit": "^12.1.0",
    "npm-force-resolutions": "0.0.10",
    "postcss": "^8.3.0",
    "rxjs": "~6.6.0",
    "ts-jest": "^27.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular/cli": "~11.2.12",
    "@angular/compiler-cli": "~11.2.13",
    "@types/jest": "^26.0.23",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "expect-playwright": "^0.4.0",
    "jest": "^27.0.4",
    "jest-axe": "^5.0.1",
    "jest-playwright-preset": "^1.6.0",
    "jest-preset-angular": "^9.0.3",
    "playwright": "^1.12.0",
    "playwright-firefox": "^1.11.1",
    "playwright-webkit": "^1.11.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
